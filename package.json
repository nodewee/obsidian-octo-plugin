{
  "name": "octo",
  "version": "0.1.5",
  "description": "Octo: Zero effort naming, tagging, and filing your Notes with AI.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "typecheck": "tsc -noEmit -skipLibCheck",
    "lint": "eslint .",
    "version": "node version-bump.mjs && git add manifest.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "file-management",
    "note-management"
  ],
  "author": "HenryZ (nodewee@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "jiti": "^2.6.1",
    "obsidian": "^1.5.7",
    "tslib": "2.4.0",
    "typescript": "5.4.5"
  }
}
